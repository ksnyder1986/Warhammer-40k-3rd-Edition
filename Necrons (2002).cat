    <selectionEntry id="06be-7c68-43af-b0c4" name="The Nightbringer" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="4d99-7daa-b467-798a" value="0.0">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="7a63-eaa2-fd49-2b4e" type="equalTo"/>
