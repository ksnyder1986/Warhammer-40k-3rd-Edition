    <selectionEntry id="6c49-6711-a182-f554" name="Death Company" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="4303-921d-7856-b360" type="max"/>
