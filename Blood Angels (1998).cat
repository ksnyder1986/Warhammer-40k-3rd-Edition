    <selectionEntry id="804d-474e-74ef-fbfe" name="Moriar the Chosen" hidden="false" collective="false" import="true" type="upgrade">
      <rules>
        <rule id="8244-9018-ccd9-2f0b" name="Tear Attack" hidden="false">
          <description>The Furioso Dreadnought rolls 2D6 + Strength as Armour Penetration against vehicles</description>
        </rule>
        <rule id="c80e-0cfb-1df0-00fc" name="Black Rage" hidden="false">
          <description>Moriar must always move towards the enemy as fast as possible, and must assault an enemy within range in the assault phase.</description>
        </rule>
        <rule id="7113-3fee-2d3b-1fd0" name="Rampage" hidden="false">
          <description>Moriar ignores any Crew Shaken and Crew Stunned results.</description>
        </rule>
      </rules>
