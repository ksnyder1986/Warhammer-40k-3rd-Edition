        <rule id="dd61-3899-d803-3413" name="Command Presence" hidden="false">
          <description>Any Imperial Guard unit within 12&quot; of Macharius takes Morale and Pinning checks using his Leadership of 10</description>
        </rule>
        <rule id="7138-1cd1-de6f-1b0e" name="Ferocious Charge" hidden="false">
