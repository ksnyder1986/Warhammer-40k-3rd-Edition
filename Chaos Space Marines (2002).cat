              <rules>
                <rule id="57ba-df57-0b0f-e268" name="Warp Amp" hidden="false">
                  <description>Enemy units within 12&quot; from the vehicle are at -1 Ld, -2 Ld within 6&quot; and -3 Ld if in contact. Does not stack with other Warp Amps.</description>
                </rule>
              </rules>
              <costs>
