        <profile id="8a0f-4f1f-bdf4-1fd1" name="Deathstrike Missile" hidden="false" typeId="b399-8a1b-bf0b-c5b7" typeName="Weapon">
          <characteristics>
            <characteristic name="Range" typeId="deb8-131d-bdcf-1376">G12&quot;-960&quot;</characteristic>
            <characteristic name="S" typeId="24d8-2110-08fa-d844">10</characteristic>
