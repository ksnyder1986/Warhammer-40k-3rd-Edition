    <selectionEntry id="fde5-3ce5-0c7a-820c" name="Hyperious Air Defence Battery (IA)" hidden="false" collective="false" import="true" type="unit">
      <rules>
        <rule id="21e9-0efc-b045-a446" name="Battery" hidden="false">
          <description>The platforms do not need to be deployed together, although each Hyperios platform must be less than 36&quot; from the command platform.</description>
        </rule>
        <rule id="82d8-3330-5084-1855" name="Firing Mode" hidden="false">
