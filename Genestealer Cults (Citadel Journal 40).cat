      <profiles>
        <profile id="7549-85db-3399-9b81" name="The Flames of Blood" hidden="false" typeId="b399-8a1b-bf0b-c5b7" typeName="Weapon">
          <characteristics>
            <characteristic name="Range" typeId="deb8-131d-bdcf-1376">Template</characteristic>
