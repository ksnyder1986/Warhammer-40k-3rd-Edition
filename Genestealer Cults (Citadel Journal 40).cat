  <sharedSelectionEntries>
    <selectionEntry id="c406-23d3-1c02-6e7f" name="Genestealer Magus" hidden="false" collective="false" import="true" type="unit">
      <rules>
        <rule id="9d2f-f8aa-24a5-7ab3" name="He Who Runs Away..." hidden="false">
          <description>The Magus can make a Voluntary Fall-back by making a successful Leadership Test, leaving his Bodyguard to keep on fighting in an Assault. He will automatically Regroup after this movement has ended, or if he reaches the edge of the board.</description>
        </rule>
      </rules>
      <infoLinks>
        <infoLink id="0271-c8ea-031a-7d04" name="Independent Character" hidden="false" targetId="4a60-7f7e-c37f-a4d6" type="rule"/>
