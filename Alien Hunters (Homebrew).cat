                <infoLink id="411a-ca6c-5b93-b1f3" name="Bolt Pistol" hidden="false" targetId="e9ac-c779-51bb-3c9a" type="profile"/>
                <infoLink id="7ca0-d56f-6fe5-97b5" name="Power Weapon" hidden="false" targetId="5774-7187-2616-dd8a" type="profile"/>
