                <infoLink id="e253-89dd-0502-e05a" name="Bolt Pistol" hidden="false" targetId="e9ac-c779-51bb-3c9a" type="profile"/>
                <infoLink id="1c1b-463e-c80e-17c4" name="Power Weapon" hidden="false" targetId="5774-7187-2616-dd8a" type="profile"/>
                <infoLink id="159a-b7f3-fd82-112b" name="Melta Bombs" hidden="false" targetId="87d0-8c40-7bcb-8cbb" type="profile"/>
